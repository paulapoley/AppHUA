        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.158" />

    <Button
        android:id="@+id/botonDiagnosticaMed"
        android:layout_width="145dp"
        android:layout_height="122dp"
        android:background="@color/rosa"
